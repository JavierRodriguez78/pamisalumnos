<h1>Página de activación.</h1>
{{#if error}}
    <div class="alert-warning">
        <h2>Upsss {{error}}</h2>
    </div>
{{/if}}
{{#unless error}}
    <div class="alert-success">
        <h2>Tu cuenta ya se encuentra activa</h2>
        <a href="http://localhost:3000/login">Login</a>
    </div>
{{/unless}}